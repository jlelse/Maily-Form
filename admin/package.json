{
  "name": "maily-form-admin",
  "version": "1.0.0",
  "description": "Admin interface for Maily-Form",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jlelse/Maily-Form.git"
  },
  "scripts": {
    "build": "npm run js-build && npm run css-build",
    "js-build": "webpack",
    "css-build": "sass sass/styles.scss dist/styles.css --no-source-map --style compressed",
    "dev": "npm-run-all -p -r js-dev css-dev",
    "js-dev": "webpack --watch",
    "css-dev": "sass --watch sass/styles.scss dist/styles.css --no-source-map --style compressed"
  },
  "devDependencies": {
    "axios": "0.18.0",
    "bulma": "0.7.2",
    "css-loader": "2.0.2",
    "npm-run-all": "4.1.5",
    "sass": "1.15.2",
    "vue": "2.5.21",
    "vue-loader": "15.4.2",
    "vue-router": "3.0.2",
    "vue-template-compiler": "2.5.21",
    "webpack": "4.28.1",
    "webpack-cli": "3.1.2"
  }
}
